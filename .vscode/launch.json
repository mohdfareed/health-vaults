{
  "configurations": [
    {
      "type": "sweetpad-lldb",
      "request": "attach",
      "name": "SweetPad: Build and Run (Wait for debugger)",
      "preLaunchTask": "sweetpad: debugging-launch"
    },
    {
      "type": "swift",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:health-vaults}",
      "name": "Debug HealthVaults",
      "program": "${workspaceFolder:health-vaults}/.build/debug/HealthVaults",
      "preLaunchTask": "swift: Build Debug HealthVaults"
    },
    {
      "type": "swift",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:health-vaults}",
      "name": "Release HealthVaults",
      "program": "${workspaceFolder:health-vaults}/.build/release/HealthVaults",
      "preLaunchTask": "swift: Build Release HealthVaults"
    },
    {
      "type": "swift",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:health-vaults}",
      "name": "Debug HealthVaultsWidgets",
      "preLaunchTask": "swift: Build Debug HealthVaultsWidgets",
      "target": "HealthVaultsWidgets",
      "configuration": "debug"
    },
    {
      "type": "swift",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:health-vaults}",
      "name": "Release HealthVaultsWidgets",
      "preLaunchTask": "swift: Build Release HealthVaultsWidgets",
      "target": "HealthVaultsWidgets",
      "configuration": "release"
    },
    {
      "type": "swift",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:health-vaults}",
      "name": "Debug HealthVaultsApp",
      "preLaunchTask": "swift: Build Debug HealthVaultsApp",
      "target": "HealthVaultsApp",
      "configuration": "debug"
    },
    {
      "type": "swift",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder:health-vaults}",
      "name": "Release HealthVaultsApp",
      "preLaunchTask": "swift: Build Release HealthVaultsApp",
      "target": "HealthVaultsApp",
      "configuration": "release"
    }
  ]
}
